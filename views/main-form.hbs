<body>
  <link rel="stylesheet" href="/css/index.css">
  <script defer src="/js/index.js"></script>
</body>

<div class="header" id="myHeader">
  <h2> Заявка </h2>
  <h2> Персонал </h2>
  <h2> Помещение </h2>
  <h2> Оборудование </h2>
  <h2> Материалы </h2>
  <h2> История </h2>
</div>

<div id="w">
  <h1>Заявка!!!</h1>
  <form id="contactform" name="contact" method="post" action="#">

    <hr align="center" size="3" color="black" width="100%">

    <h2 class="keyWord"> Основное </h2>
    <div class="row">
      <label for="name"> Инициатор <span class="req">{{name}}</span></label>
      <input type="text" name={{id}} id="string" class="txt" tabindex="1" placeholder="Ваше имечко" required>
    </div>

    <hr align="center" size="2" width="100%">

    <div class="row">
      <label for="email"> Медицинский центр<span class="req">*</span></label>
      <select type="text" name="medcenter" id="string" class="txt" tabindex="2" size="1" required>
        <option value="none" hidden="">Выберите тип</option>
        <option value="1">p1</option>
        <option value="2">p2</option>
      </select>
    </div>

    <hr align="center" size="2" width="100%">

    <div class="row">
      <label for="subject"> Подразделение <span class="req">*</span></label>
      <select type="text" name="division" id="string" class="txt" tabindex="3" size="1" required>
        <option value="none" hidden="">Выберите тип</option>
        <option value="1">p1</option>
        <option value="2">p2</option>
      </select>
    </div>

    <hr align="center" size="2" width="100%">

    <div class="row">
      <label for="message"> Специализация <span class="req">*</span></label>
      <select type="text" name="specialisation" id="string" class="txt" tabindex="4" size="1" required>
        <option value="none" hidden="">Выберите тип</option>
        <option value="1">p1</option>
        <option value="2">p2</option>
      </select>
    </div>

    <hr align="center" size="2" width="100%">

    <div class="row">
      <label for="message"> Анестезия <span class="req">*</span></label>
      <select type="text" name="anestasia" id="string" class="txt" tabindex="5" size="1" required>
        <option value="none" hidden="">Выберите тип</option>
        <option value="1">Да</option>
        <option value="2">Нет</option>
      </select>
    </div>

    <hr align="center" size="2" width="100%">

    <div class="row">
      <label for="message"> Вид анестезии <span class="req">*</span></label>
      <select type="text" name="anestasiaType" id="string" class="txt" tabindex="5" size="1" required>
        <option value="none" hidden="">Выберите тип</option>
        <option value="1">p1</option>
        <option value="2">p2</option>
      </select>
    </div>

    <hr align="center" size="3" color="black" width="100%"> {{!--------- -------------}}

    <div class="row">
      <label for="name"> Код услуги <span class="req"></span></label>
      <input type="text" name="serviceCode" id="string" class="txt" tabindex="6" placeholder="Ваше имечко" required>
    </div>

    <hr align="center" size="2" width="100%">


    <div class="row">
      <label for="name"> Название услуги на русском <span class="req">*</span></label>
      <input type="text" name="serviceNameRus" id="string" class="txt" tabindex="7" placeholder="Ваше имечко" required>
    </div>

    <hr align="center" size="2" width="100%">


    <div class="row">
      <label for="name"> Название услуги на английском <span class="req"></span></label>
      <input type="text" name="serviceNameEng" id="string" class="txt" tabindex="8" placeholder="Ваше имечко" required>
    </div>

    <hr align="center" size="2" width="100%">


    <div class="row">
      <label for="name"> Стоимость, у.е. <span class="req">*</span></label>
      <input type="text" name="price" id="string" class="txt" tabindex="9" placeholder="Ваше имечко" required>
    </div>

    <hr align="center" size="2" width="100%">


    <div class="row">
      <label for="message"> Субподряд <span class="req">*</span></label>
      <select type="text" name="subpodryad" id="string" class="txt" tabindex="10" size="1" required>
        <option value="none" hidden="">Выберите тип</option>
        <option value="1">Да</option>
        <option value="2">Нет</option>
      </select>
    </div>

    <hr align="center" size="2" width="100%">


    <div class="row">
      <label for="message"> Субподрядная организация <span class="req">*</span></label>
      <select type="text" name="subpodryadOrg" id="string" class="txt" tabindex="11" size="1" required>
        <option value="none" hidden="">Выберите тип</option>
        <option value="1">1</option>
        <option value="2">p2</option>
      </select>
    </div>

    <hr align="center" size="2" width="100%">


    <div class="row">
      <label for="name"> Стоимость субподряда <span class="req">*</span></label>
      <input type="number" name="priceSubpodryad" id="string" class="txt" tabindex="12" placeholder="Ваше имечко" required>
    </div>

    <hr align="center" size="2" width="100%">


    <div class="row">
      <label for="name"> Кол-во услуг в месяц <span class="req"></span></label>
      <input type="number" name="numberOfServices" id="string" class="txt" tabindex="13" placeholder="Ваше имечко" required>
    </div>

    <hr align="center" size="2" width="100%">


    <div class="row">
      <label for="name"> Аналог услуги <span class="req"></span></label>
      <input type="text" name="serviceAnalog" id="string" class="txt" tabindex="14" placeholder="Ваше имечко" required>
    </div>

    <hr align="center" size="2" width="100%">

    <div class="row">
      <label for="name"> Комментарии <span class="req"></span></label>
      <input type="text" name="comment" id="string" class="txt" tabindex="16" placeholder="Ваше имечко" required>
    </div>

    <hr align="center" size="2" width="100%">

    <div class="row">
      <label for="name"> Дата запуска услуги <span class="req"></span></label>
      <input type="date" name="dateOfStart" id="string" class="txt" tabindex="17" placeholder="Ваше имечко" required>
    </div>

    <hr align="center" size="2" width="100%">

    <hr align="center" size="3" color="black" width="100%">
    <hr align="center" size="3" color="black" width="100%">
    {{!--------------------------------------------------- -----------------------------------------------------}}

    <h2 class="keyWord"> Трудозатраты </h2>
    <div>
      <div class="row">
        <select type="text" name="laborCost[executor]" id="string" class="txt" tabindex="11" size="1" required>
          <option value="none" hidden="">Выберите исполнителя </option>
          <option value="1">p1</option>
          <option value="2">p2</option>
        </select>
        <input type="number" name="laborCost[time]" id="string" class="txt" tabindex="17" placeholder="Время работы, мин" required
          size="30">
      </div>

      <hr align="center" size="2" width="100%">

      <input type="submit" name="laborCost[added]"  id="string" class="txt" tabindex="17" value="+ добавить поле ">
    </div>

    <hr align="center" size="2" width="100%">

    <h4 class="handWord"> Если помещение не удается найти в списке, пожалуйста, введите вручную в поля ниже:
    </h4>

    <hr align="center" size="3" color="black" width="100%">


    <div class="row">
      <input type="text" name="laborCost[executorOutOfList]" id="string" class="txt" tabindex="17" placeholder="Исполнитель" required>
      <input type="text" name="LaborCost[timeOutOfList]" id="string" class="txt" tabindex="17" placeholder="Время работы, мин" required
        size="30">
    </div>

    <hr align="center" size="2" width="100%">

    <input type="submit" name="laborCost[addOutOfList]" id="string" class="txt" tabindex="17" value="+ добавить поле ">

    <hr align="center" size="2" width="100%">

    {{!------------------------------------------------------ ---------------------------------------------------------------}}

    <h2 class="keyWord">  Помещения </h2>
    <div>
      <div class="row">
        <select type="text" name="premises[inner]" id="string" class="txt" tabindex="11" size="1" required>
          <option hidden="">Выберите Помещение </option>
          <option value="1">p1</option>
          <option value="2">p2</option>
        </select>
        <input type="number" name="premises[time]" id="string" class="txt" tabindex="17"
          placeholder="Время работы в помещении, мин" required size="30">
      </div>

      <hr align="center" size="2" width="100%">

      <input type="submit" name="premises[added]" id="string" class="txt" tabindex="17" value="+ добавить поле ">
    </div>

    <hr align="center" size="2" width="100%">

    <h4 class="handWord"> Если исполнителя не удается найти в списке, пожалуйста, введите вручную в поля ниже:
    </h4>

    <hr align="center" size="3" color="black" width="100%">


    <div class="row">
      <input type="text" name="premises[handwrite]" id="string" class="txt" tabindex="17" placeholder="Помещение" required>
      <input type="text" name="premises[handwriteTime]" id="string" class="txt" tabindex="17" placeholder="Время работы в помещении, мин"
        required size="30">
    </div>

    <hr align="center" size="2" width="100%">

    <input type="submit" name="name" id="string" class="txt" tabindex="17" value="+ добавить поле ">

    <hr align="center" size="2" width="100%">

    {{!------------------------------------------------------ ---------------------------------------------------------------}}

    <h2 class="keyWord"> Оборудование </h2>
    <div>
      <div class="row">
        <select type="text" name="equipment[choosed]" id="string" class="txt" tabindex="11" size="1" required>
          <option value="none" hidden="">Выберите оборудование </option>
          <option value="1">p1</option>
          <option value="2">p2</option>
        </select>
        <input type="number" name="equipmet[time]" id="string" class="txt" tabindex="17"
          placeholder="Время работы на оборудовании, мин" required size="30">
      </div>

      <hr align="center" size="2" width="100%">

      <input type="submit" name="equipement[added]" id="string" class="txt" tabindex="17" value="+ добавить поле ">
    </div>

    <hr align="center" size="2" width="100%">

    <h4 class="handWord"> Если оборудование не удается найти в списке, пожалуйста, введите вручную в поля ниже:
    </h4>

    <hr align="center" size="3" color="black" width="100%">


    <div class="row">
      <input type="text" name="equipement[handwrite]" id="string" class="txt" tabindex="17" placeholder="Оборудование" required>
      <input type="text" name="equipement[handwriteTime]" id="string" class="txt" tabindex="17"
        placeholder="Время работы на оборудовании, мин" required size="30">
    </div>

    <hr align="center" size="2" width="100%">

    <input type="submit" name="equipemint[handwriteAdded]" id="string" class="txt" tabindex="17" value="+ добавить поле ">

    <hr align="center" size="2" width="100%">

    {{!------------------------------------------------------ ---------------------------------------------------------------}}

    <h2 class="keyWord"> Расходные Материалы </h2>
    <div id="materialUp" class="row">


      <select type="text" name="materials[choosed]" id="string" class="txt" tabindex="11" size="1" required>
        <option value="none" hidden="">Выберите Материал </option>
        <option value="1">p1</option>
        <option value="2">p2</option>
      </select>

      <select type="text" name="materials[using]" id="string" class="txt" tabindex="10" size="1" required>
        <option value="none" hidden="">Кол-во использование </option>
        <option value="1">Многоразовое</option>
        <option value="2">Одноразовое</option>
      </select>
      <input type="number" name="materials[much]" id="string" class="txt" tabindex="17" placeholder="Кол-во" required size="30">

      <select type="text" name="materials[meter]" id="string" class="txt" tabindex="11" size="1" required>
        <option value="none" hidden=""> Еденица измерения </option>
        <option value="1">км</option>
        <option value="2">м</option>
      </select>

    </div>

    <hr align="center" size="2" width="100%">

    <input type="submit" name="material[handmade]" id="string" class="txt" tabindex="17" value="+ добавить поле ">

    <hr align="center" size="2" width="100%">

    <h4 class="handWord"> Если исполнителя не удается найти в списке, пожалуйста, введите вручную в поля ниже:
    </h4>

    <hr align="center" size="3" color="black" width="100%">


    <div id="materialUp" class="row">
      <input type="number" name="material[handmade]" id="string" class="txt" tabindex="17" placeholder="Материал" required size="30">

      <select type="text" name="material[handmadeUsing]" id="string" class="txt" tabindex="10" size="1" required>
        <option value="none" hidden="">Кол-во использование </option>
        <option value="1">Многоразовое</option>
        <option value="2">Одноразовое</option>
      </select>
      <input type="number" name="material[handmadeMuch]" id="string" class="txt" tabindex="17" placeholder="Кол-во" required size="30">

      <select type="text" name="material[handmadeMeter]" id="string" class="txt" tabindex="11" size="1" required>
        <option value="none" hidden=""> Еденица измерения </option>
        <option value="1">км</option>
        <option value="2">м</option>
      </select>
    </div>

    <hr align="center" size="2" width="100%">

    <input type="submit" name="material[handmadeAdded]" id="string" class="txt" tabindex="17" value="+ добавить поле ">

    <hr align="center" size="2" width="100%">


    {{!------------------------------------------------------ ---------------------------------------------------------------}}


  </form>
</div>

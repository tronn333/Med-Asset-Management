<body>
  <link rel="stylesheet" href="/css/index.css">
  <script defer src="/js/index.js"></script>
</body>

<div class="header" id="myHeader">
  <h2><a class="head" href="#title1" style="color: black;">Заявка </a> </h2>
  <h2><a class="head" href="#title2" style="color: black;">Персонал </a> </h2>
  <h2><a class="head" href="#title3" style="color: black;">Помещение </a> </h2>
  <h2><a class="head" href="#title4" style="color: black;">Оборудование </a> </h2>
  <h2><a class="head" href="#title5" style="color: black;">Материалы </a> </h2>
  <h2><a class="head" href="/entry/{{entryId}}/history" style="color: black;">История </a> </h2>
  <h2><a class="head" href="/homepage" style="color: black;">Домой </a> </h2>
</div>

<div id="w">
  <form id="contactform" name="contact" {{#each application}} {{#if status}} method='POST' action='/updateentry/{{_id}}'
    {{else}} method="post" action="#" {{/if}} {{/each}}>

    <hr align="center" size="3" color="black" width="100%">


    <div id="block">
      <h2 class="keyWord">Основное </h2>
      <div class="column">
        <div class="row">
          <label for="name"> Инициатор <span id="name" class="req">{{name}}</span></label>
          <input type="hidden" name={{id}} id="string" class="txt" tabindex="1">
        </div>
        {{#each application}}
        <hr align="center" size="2" width="100%">
        <div class="row">
          <label for="email"> Медицинский центр<span class="req">*</span></label>
          <select type="text" name="medcenter" id="string" class="txt" tabindex="2" value='{{this.medcenter}}' required
            size="1" style="width: 313px;">
            <option value="none" hidden="">{{#if medcenter}} '{{medcenter}}' {{else}} Выберите тип{{/if}}</option>
            <option value="1">p1</option>
            <option value="2">p2</option>
          </select>
        </div>

        <hr align="center" size="2" width="100%">

        <div class="row">
          <label for="subject"> Подразделение <span class="req">*</span></label>
          <select type="text" name="division" style="width: 313px;" id="string" class="txt" value='{{this.division}}'
            required tabindex="3" size="1">
            <option value="none" hidden="">{{#if this.division}} '{{this.division}}' {{else}} Выберите тип{{/if}}
            </option>
            <option value="1">p1</option>
            <option value="2">p2</option>
          </select>
        </div>


        <hr align="center" size="2" width="100%">


        <div class="row">
          <label for="message"> Специализация <span class="req">*</span></label>
          <select type="text" name="specialisation" style="width: 313px;" id="string" class="txt" required
            value='{{this.specialisation}}' tabindex="4" size="1">
            <option value="none" hidden="">{{#if this.specialisation}} '{{this.specialisation}}' {{else}} Выберите
              тип{{/if}}</option>
            <option value="1">p1</option>
            <option value="2">p2</option>
          </select>
        </div>


        <hr align="center" size="2" width="100%">

        <div class="row">
          <label for="message"> Анестезия <span class="req">*</span></label>
          <select type="text" name="anestasia" style="width: 313px;" id="string" class="txt" required
            value='{{anestasia}}' tabindex="5" size="1">
            <option value="none" hidden="">{{#if this.anestasia}} '{{this.anestasia}}' {{else}} Выберите тип{{/if}}
            </option>
            <option value="1">Да</option>
            <option value="2">Нет</option>
          </select>
        </div>


        <hr align="center" size="2" id="anestesia1" width="100%" style="display: none;">

        <div class="row" id="anestesia" style="display: none;">
          <label for="message"> Вид анестезии <span class="req">*</span></label>
          <select type="text" name="anestasiaType" id="string" class="txt" required value='{{anestesiaType}}'
            tabindex="5" size="1">
            <option value="none" hidden="">{{#if anestasiaType}} '{{anestasiaType}}' {{else}} Выберите тип{{/if}}
            </option>

            <option value="1">p1</option>
            <option value="2">p2</option>
          </select>
        </div>

        <hr align="center" size="2" width="100%">
        {{!--
        <hr align="center" size="3" color="black" width="100%"> --}}

        <div class="row">
          <label for="name"> Код услуги <span class="req"></span></label>
          <input type="text" name="serviceCode" value='{{this.serviceCode}}' {{#if neadmin}} readonly {{/if}}
            id="string" class="txt" tabindex="6" placeholder="Код услуги">
        </div>


        <hr align="center" size="2" width="100%">



        <div class="row">
          <label for="name"> Название услуги на русском <span class="req">*</span></label>
          <input type="text" name="serviceNameRus" id="string" value='{{this.serviceNameRus}}' required class="txt"
            tabindex="7" placeholder="на русском">
        </div>

        <hr align="center" size="2" width="100%">



        <div class="row">
          <label for="name"> Название услуги на английском <span class="req"></span></label>
          <input type="text" name="serviceNameEng" value='{{this.serviceNameEng}}' id="string" class="txt" tabindex="8"
            placeholder="на английском">
        </div>

        <hr align="center" size="2" width="100%">



        <div class="row">
          <label for="name"> Стоимость, у.е. <span class="req">*</span></label>
          <input type="text" name="price" id="string" value='{{this.price}}' required {{#if neadmin}} readonly {{/if}}
            class="txt" tabindex="9" placeholder="Стоимость">
        </div>

        <hr align="center" size="2" width="100%">



        <div class="row">
          <label for="message"> Субподряд <span class="req">*</span></label>
          <select type="text" name="subpodryad" id="string" required class="txt" tabindex="10" size="1"
            style="width: 313px;">
            <option value="none" hidden="">{{#if subpodryad}} '{{subpodryad}}' {{else}} Выберите тип{{/if}}</option>
            <option value="1">Да</option>
            <option value="2">Нет</option>
          </select>
        </div>


        <hr align="center" style="display: none;" id="subpodryad1" size="2" width="100%">




        <div class="row" id="subpodryad2" style="display: none;">
          <label for="message"> Субподрядная организация <span class="req">*</span></label>
          <select type="text" name="subpodryadOrg" style="width: 313px;" id="string" value='{{thissubpodryadOrg}}'
            required class="txt" tabindex="11" size="1">
            <option value="none" hidden="">Выберите тип</option>
            <option value="1">1</option>
            <option value="2">p2</option>
          </select>
        </div>

        <hr align="center" size="2" width="100%" style="display: none;" id="subpodryad3">



        <div class="row" id="subpodryad4" style="display: none;">
          <label for="name"> Стоимость субподряда <span class="req">*</span></label>
          <input type="number" name="priceSubpodryad" value='{{this.priceSubpodryad}}' id="string" required class="txt"
            tabindex="12" placeholder="субподряд">
        </div>

        <hr align="center" size="2" width="100%">



        <div class="row">
          <label for="name"> Кол-во услуг в месяц <span class="req"></span></label>
          <input type="number" name="numberOfServices" id="string" value='{{this.numberOfServices}}' class="txt"
            tabindex="13" placeholder=" Кол-во услуг">
        </div>

        <hr align="center" size="2" width="100%">



        <div class="row">
          <label for="name"> Аналог услуги <span class="req"></span></label>
          <input type="text" name="serviceAnalog" id="string" value='{{this.serviceAnalog}}' class="txt" tabindex="14"
            placeholder="Аналог услуги">
        </div>

        <hr align="center" size="2" width="100%">

        <div class="row">
          <label for="name"> Комментарии <span class="req"></span></label>
          <textarea type="text" name="comment" id="string" class="txt" tabindex="15"
            placeholder="Комментарии"></textarea>
        </div>

        <hr align="center" size="2" width="100%">


        <div class="row">
          <label for="name"> Дата запуска услуги <span class="req"></span></label>
          <input type="date" name="dateOfStart" value='{{this.dateOfStart}}' id="string" class="txt" tabindex="17"
            placeholder="Дата">
        </div>

        <hr align="center" size="2" width="100%">
      </div>

    </div><a name='title2'> </a>

    <hr align="center" size="3" color="black" width="100%" class="indent">
    <hr align="center" size="3" color="black" width="100%">
    {{!--------------------------------------------------- -----------------------------------------------------}}
    <div id="block">
      <h2 class="keyWord"> Трудозатраты </h2>
      <div class="column">
        <div>
          {{#if laborcost.choosed.length}}
          {{#each laborcost.choosed}}
          <select type="text" name="laborCost[choosed]" id="string" class="txt" tabindex="11" size="1">
            <option value="none" hidden="">{{#if this}} '{{this}}' {{else}} Выберите
              Исполнителя{{/if}} </option>
            <option value="1">p1</option>
            <option value="2">p2</option>
          </select>
          <input type="number" name="laborCost[time]" value='{{laborCost.time}}' id="string" class="txt" tabindex="17"
            placeholder="Время работы, мин" size="30">
        </div>
        {{/each}}
        {{else}}
        <div class="row">
          <select type="text" name="laborCost[choosed]" value='{{this.laborCost.choosed}}' id="string" class="txt"
            tabindex="11" size="1" style="width: 400px;">
            <option value="none" hidden="">{{#if laborCost.choosed}} '{{laborCost.choosed}}' {{else}} Выберите
              Исполнителя{{/if}} </option>
            <option value="1">p1</option>
            <option value="2">p2</option>
          </select>
          <input type="number" name="laborCost[time]" style="width: 400px;" value='{{laborCost.time}}' id="string"
            class="txt4" tabindex="17" placeholder="Время работы, мин" size="30">
        </div>
        {{/if}}

        <hr align="center" size="2" width="100%">

        <button id="stringAdd" class="addplus" tabindex="17"> + Добавить поле</button>
      </div>
      <hr align="center" size="2" width="100%" class="height">


      <h4 class="handWord"> Если исполнителя не удается найти в списке, пожалуйста, введите вручную в поля ниже:
      </h4>

      {{!--
      <hr align="center" size="3" color="black" width="100%"> --}}

      <div>
        <div class="row">
          <input type="text" name="laborCost[handWrite]" style="width: 375px;" value='{{laborCost.handWrite}}'
            id="string" class="txt4" tabindex="17" placeholder="Исполнитель">
          <input type="text" name="laborCost[timeHandWrite]" style="width: 400px;" value='{{laborCost.timeHandWrite}}'
            id="string" class="txt4" tabindex="17" placeholder="Время работы, мин" size="30">
        </div>

        <hr align="center" size="2" width="100%">

        <button id="stringAdd" class="addplus" tabindex="17">+ Добавить поле</button>
      </div>
      <hr align="center" size="2" width="100%" class="height">
    </div>
</div><a name='title3'> </a>


<hr align="center" size="3" color="black" width="100%" class="indent">
<hr align="center" size="3" color="black" width="100%">
